vrrp_instance VI_1 {
    state {{ keepalived_state }}        # MASTER or BACKUP
    interface {{ keepalived_interface }}  # ens33 or other interface
    virtual_router_id 51
    priority {{ keepalived_priority }}  # 100 for MASTER, 90 for BACKUP
    advert_int 1
    authentication {
        auth_type PASS
        auth_pass secret
    }
    virtual_ipaddress {
        192.168.159.140
    }
}
